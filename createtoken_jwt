#!/bin/bash

read -p "Authorization: Bearer : " auth
echo
echo "createtoken jwt /signin"
read -p "Username : " username
read -p "pass : " pass

eval "curl -d '{\"username\": \"$username\",\"pass\": \"$pss\"}' -H 'Content-Type: application/json' -H 'Authorization: Bearer $auth' -X POST http://localhost:7000/signin | jq '.'"
